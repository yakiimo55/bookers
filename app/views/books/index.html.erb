<h1>Books</h1>



<table>
  <thead>
    <tr>
      <th>Title</th>  <th>Body</th>  <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>リーダブルコード</td>
      <td>より良いコードを書くためのシンプルで実践的なテクニック</td>
      <td>Show</td>
      <td>Edit</td>
      <td>Destory</td>
    </tr>
  </tbody>
</table>


<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  
  <h4>Body</h4>
  <%= f.text_area :body %>
  
  <%= f.submit 'create Book' %>  <%#'   'マーク不要？%>
<% end %>
  